<div class="row">
  <div class="col-sm-2">
    <% if current_user %>
      <%= link_to "Quack", new_quack_path, id: 'new_link', remote: true, class: "btn btn-success" %>
    <% else %>
      <h5>You must login in order to quack.</h5>
    <% end %>
  </div>
  <div class="col-sm-8" id="quack-list">
    <div>
      <% if @quacks.any? %>
      <%= render partial: "quack", collection: @quacks %>


        
      <% else %>
        Get quacking!
      <% end %>
    </div>
  </div>
  <div class="col-sm-2"></div>

</div>
